<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeEditText">

    <fragment
        android:id="@+id/homeEditText"
        android:name="com.example.fc_online.ui.fragment.HomeEditText"
        android:label="fragment_home_edit_text"
        tools:layout="@layout/fragment_home_edit_text" >
        <action
            android:id="@+id/action_homeEditText_to_userInfoText"
            app:destination="@id/userInfoText" />
        <action
            android:id="@+id/action_homeEditText_to_failSearch"
            app:destination="@id/failSearch" />
    </fragment>
    <fragment
        android:id="@+id/userInfoText"
        android:name="com.example.fc_online.ui.fragment.UserInfoText"
        android:label="fragment_user_info_text"
        tools:layout="@layout/fragment_user_info_text" >
        <action
            android:id="@+id/action_userInfoText_to_tradeRecord"
            app:destination="@id/tradeRecord" />
    </fragment>
    <fragment
        android:id="@+id/failSearch"
        android:name="com.example.fc_online.ui.fragment.FailSearch"
        android:label="fragment_fail_search"
        tools:layout="@layout/fragment_fail_search" >
        <action
            android:id="@+id/action_failSearch_to_homeEditText"
            app:destination="@id/homeEditText" />
    </fragment>
    <fragment
        android:id="@+id/tradeRecord"
        android:name="com.example.fc_online.ui.fragment.TradeRecord"
        android:label="fragment_trade_record"
        tools:layout="@layout/fragment_trade_record" >
        <action
            android:id="@+id/action_tradeRecord_to_tradeRecordBuy"
            app:destination="@id/tradeRecordBuy" />
        <action
            android:id="@+id/action_tradeRecord_to_tradeRecordSell"
            app:destination="@id/tradeRecordSell" />
        <action
            android:id="@+id/action_tradeRecord_to_tradeRecordMax"
            app:destination="@id/tradeRecordMax" />
        <action
            android:id="@+id/action_tradeRecord_to_tradeRecordMin"
            app:destination="@id/tradeRecordMin" />
    </fragment>
    <fragment
        android:id="@+id/tradeRecordBuy"
        android:name="com.example.fc_online.ui.fragment.TradeRecordBuy"
        android:label="fragment_trade_record_buy"
        tools:layout="@layout/fragment_trade_record_buy" />
    <fragment
        android:id="@+id/tradeRecordSell"
        android:name="com.example.fc_online.ui.fragment.TradeRecordSell"
        android:label="fragment_trade_record_sell"
        tools:layout="@layout/fragment_trade_record_sell" />
    <fragment
        android:id="@+id/tradeRecordMax"
        android:name="com.example.fc_online.ui.fragment.TradeRecordMax"
        android:label="fragment_trade_record_max"
        tools:layout="@layout/fragment_trade_record_max" />
    <fragment
        android:id="@+id/tradeRecordMin"
        android:name="com.example.fc_online.ui.fragment.TradeRecordMin"
        android:label="fragment_trade_record_min"
        tools:layout="@layout/fragment_trade_record_min" />
</navigation>